@ICON_RESOURCE_INFO@

1 VERSIONINFO
 FILEVERSION @FILEATTR_VER_MAJOR@,@FILEATTR_VER_MINOR@,@FILEATTR_VER_PATCH@,0
 PRODUCTVERSION @FILEATTR_VER_MAJOR@,@FILEATTR_VER_MINOR@,@FILEATTR_VER_PATCH@,0
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Barco N.V."
            VALUE "FileDescription", "@FILEATTR_DESC@"
            VALUE "FileVersion", "@FILEATTR_VER_MAJOR@.@FILEATTR_VER_MINOR@.@FILEATTR_VER_PATCH@.0"
            VALUE "InternalName", "@FILEATTR_ORIGINAL_NAME@"
            VALUE "LegalCopyright", "Barco, @FILEATTR_YEAR_RELEASE@"
            VALUE "OriginalFilename", "@FILEATTR_ORIGINAL_NAME@"
            VALUE "ProductName", "@FILEATTR_NAME@"
            VALUE "ProductVersion", "@FILEATTR_VER_MAJOR@.@FILEATTR_VER_MINOR@.@FILEATTR_VER_PATCH@.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200
    END
END
